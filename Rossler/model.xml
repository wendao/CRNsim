<Model>
  <Description>Rossler (chaotic)</Description>
  <NumberOfReactions>7</NumberOfReactions>
  <NumberOfSpecies>3</NumberOfSpecies>

  <SpeciesList>
     <Species>
       <Id>X1</Id>
       <InitialPopulation>16000</InitialPopulation>
     </Species>
     <Species>
       <Id>X2</Id>
