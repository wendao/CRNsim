    </Reaction>
    <Reaction>
      <Id>R6</Id>
      <Description> X3 -> 2X3 </Description>
      <Type>mass-action</Type>
      <Rate>0.0165</Rate>
      <Reactants>
        <SpeciesReference id="X3" stoichiometry="1"/>
      </Reactants>
      <Products>
        <SpeciesReference id="X3" stoichiometry="2"/>
      </Products>
    </Reaction>
    <Reaction>
      <Id>R7</Id>
      <Description> 2X3 -> X3 </Description>
      <Type>mass-action</Type>
      <Rate>5e-9</Rate>
      <Reactants>
        <SpeciesReference id="X3" stoichiometry="2"/>
      </Reactants>
      <Products>
        <SpeciesReference id="X3" stoichiometry="1"/>
      </Products>
    </Reaction>
  </ReactionsList>
